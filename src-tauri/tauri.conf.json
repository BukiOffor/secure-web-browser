{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Secure Browser",
  "version": "0.1.0",
  "identifier": "com.multilent.dev",
  "build": {
    "frontendDist": "out"
  },
  "app": {
    "windows": [
      {
        "title": "Browser",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "https://admin-examalpha.netlify.app"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
